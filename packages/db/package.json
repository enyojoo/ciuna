{
  "name": "@ciuna/db",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:reset": "supabase db reset",
    "db:seed": "tsx src/seed.ts",
    "db:migrate": "supabase db push",
    "db:generate-types": "supabase gen types typescript --local > src/generated/types.ts"
  },
  "dependencies": {
    "@ciuna/config": "workspace:*",
    "@supabase/supabase-js": "^2.38.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "tsx": "^4.6.0",
    "supabase": "^1.123.0"
  }
}
